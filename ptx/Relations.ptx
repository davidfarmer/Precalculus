      <ol cols="5">
        <li xml:id="relationsetfirst">
          <p>
            <m>~</m> \begin{mfpic}[15]{-5}{2}{-2}{5} \point[4pt]{(-4, -1), (-2, 1), (0, 3), (1, 4)} \axes \tlabel[cc](2,-0.5){<m>x</m>} \tlabel[cc](0.5,5){<m>y</m>} \xmarks{-4,-3,-2,-1,1} \ymarks{-1,1,2,3,4} \tlpointsep{5pt} \axislabels {x}{{<m>-4</m>} -4, {<m>-3</m>} -3, {<m>-2</m>} -2, {<m>-1</m>} -1, {<m>1</m>} 1} \axislabels {y}{{<m>-1</m>} -1, {<m>1</m>} 1, {<m>2</m>} 2, {<m>3</m>} 3, {<m>4</m>} 4} \tcaption{Relation <m>A</m>} \end{mfpic} \columnbreak
