      -10000 \amp   -0.0001 \\  \hline
      -1000 \amp  -0.001  \\  \hline
      -100 \amp  -0.01  \\  \hline
      0 \amp  \text{ undefined }  \\  \hline
      100 \amp  0.01  \\  \hline
